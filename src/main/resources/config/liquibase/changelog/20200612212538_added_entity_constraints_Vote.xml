<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Vote.
    -->
    <changeSet id="20200612212538-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="question_id"
                                 baseTableName="vote"
                                 constraintName="fk_vote_question_id"
                                 referencedColumnNames="id"
                                 referencedTableName="question"/>

        <addForeignKeyConstraint baseColumnNames="answer_id"
                                 baseTableName="vote"
                                 constraintName="fk_vote_answer_id"
                                 referencedColumnNames="id"
                                 referencedTableName="answer"/>

    </changeSet>
</databaseChangeLog>
